<?xml version="1.0" encoding="utf-8" ?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">




    <bean id="actor" class="com.stackroute.domain.Actor">

    <property name="actorName" value="Ranveer Singh" />
    <property name="gender" value="Male" />
    <property name="age" value="36" />

</bean>

    <bean id="movie" class="com.stackroute.domain.Movie">
        <property name="actorRef" ref="actor" ></property>

    </bean>


</beans>
